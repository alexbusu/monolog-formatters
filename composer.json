{
  "name": "alexbusu/monolog-formatters",
  "description": "Custom formatters for Monolog logger for PHP.",
  "keywords": [
    "logging",
    "formatters"
  ],
  "type": "library",
  "require": {
    "php": "^7.2 || ~8.0.0 || ~8.1.0",
    "ext-json": "*",
    "monolog/monolog": "^1.3.0 || ^2"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5 || ^9.0",
    "psalm/phar": "^4.7"
  },
  "autoload": {
    "psr-4": {
      "Alexbusu\\Monolog\\Formatter\\": "src/Formatter"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Alexbusu\\Monolog\\Formatter\\": "tests/unit/Formatter"
    }
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Alexandru Busuioc",
      "email": "busuioc.alexandru@gmail.com"
    }
  ]
}
